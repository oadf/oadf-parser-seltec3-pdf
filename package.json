    "babel-register": "^6.22.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
